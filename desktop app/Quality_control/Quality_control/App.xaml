<Application x:Class="Quality_control.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Quality_control"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="styles/Colors.xaml"></ResourceDictionary>
                <ResourceDictionary Source="styles/Label.xaml"></ResourceDictionary>
                <ResourceDictionary Source="styles/LabelData.xaml"></ResourceDictionary>
                <ResourceDictionary Source="styles/ButtonNO.xaml"></ResourceDictionary>
                <ResourceDictionary Source="styles/ButtonYES.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
